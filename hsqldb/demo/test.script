CREATE TABLE OWNERSHIP(WORK INTEGER,MUSEUM INTEGER)
CREATE TABLE ARTIST(ANO INTEGER NOT NULL IDENTITY PRIMARY KEY,NAME VARCHAR(500),BIRTHDATE INTEGER,DEATHDATE INTEGER)
CREATE TABLE LAZY_ROLENODE(ROLEID VARCHAR(12) NOT NULL,NODEID VARCHAR(100) NOT NULL,TYPEID VARCHAR(12) NOT NULL,DISPLAY VARCHAR(12),SEQ INTEGER,LIB VARCHAR(100),CONSTRAINT SYS_PK_LAZY_ROLENODE PRIMARY KEY(ROLEID,NODEID,TYPEID))
